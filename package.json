  "keywords": [
    "react",
    "design-system",
    "components",
    "ui",
    "atomic-design",
    "data-layer"
  ],
  "author": "Avin1998",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Avin1998/design_system.git"
  },
  "bugs": {
    "url": "https://github.com/Avin1998/design_system/issues"
  },

  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "dependencies": {
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.7.1"
  },
  "devDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "browserslist": {
